file = {:Users/Aushickman/Documents/Cornell/Papers/QWHEMT{\_}breakdown{\_}bib/Polarization-Induced GaN-on-Insulator E$\backslash$:D Mode p-Channel Heterostructure FETs.pdf:pdf},
issn = {07413106},
journal = {IEEE Electron Device Letters},
keywords = {AlN,GaN,enhancement and depletion (E/D) mode,molecular beam epitaxy (MBE),p-channel,polarization,transistor},
number = {7},
pages = {852--854},
