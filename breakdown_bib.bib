@article{Li2014,
abstract = {Double heterostructures of strained GaN quantum wells (QWs) sandwiched between relaxed AlN layers provide a platform to investigate the quantum-confined electronic and optical properties of the wells. The growth of AlN/GaN/AlN heterostructures with varying GaN quantum well thicknesses on AlN by plasma molecular beam epitaxy (MBE) is reported. Photoluminescence spectra provide the optical signature of the thin GaN QWs. Reciprocal space mapping in X-ray diffraction shows that a GaN layer as thick as ∼28 nm is compressively strained to the AlN layer underneath. The density of the polarization-induced two-dimensional electron gas (2DEG) in the undoped heterostructures increases with the GaN QW thickness, reaching ∼2.5 × 1013/cm2. This provides a way to tune the 2DEG channel density without changing the thickness of the top barrier layer. Electron mobilities less than ∼400 cm2/Vs are observed, leaving ample room for improvement. Nevertheless, owing to the high 2DEG density, strained GaN QW field-effect transistors with MBE regrown ohmic contacts exhibit an on-current density ∼1.4 A/mm, a transconductance ∼280 mS/mm, and a cut off frequency f T ∼ 104   GHz for a 100-nm-gate-length device. These observations indicate high potential for high-speed radio frequency and high voltage applications that stand to benefit from the extreme-bandgap and high thermal conductivity of AlN.},
author = {Li, Guowang and Song, Bo and Ganguly, Satyaki and Zhu, Mingda and Wang, Ronghua and Yan, Xiaodong and Verma, Jai and Protasenko, Vladimir and {Grace Xing}, Huili and Jena, Debdeep},
doi = {10.1063/1.4875916},
file = {:Users/Aushickman/Documents/Cornell/Papers/QWHEMT{\_}breakdown{\_}bib/Two-dimensional electron gases in strained quantum wells for AlN$\backslash$:GaN$\backslash$:AlN double heterostructure field-effect transistors on AlN.pdf:pdf},
issn = {00036951},
journal = {Applied Physics Letters},
number = {19},
title = {{Two-dimensional electron gases in strained quantum wells for AlN/GaN/AlN double heterostructure field-effect transistors on AlN}},
volume = {104},
year = {2014}
}
@article{Chaudhuri2018,
abstract = {The long-missing polarization-induced two-dimensional hole gas is finally observed in undoped gallium nitride quantum wells. Experimental results provide unambiguous proof that a 2D hole gas in GaN grown on AlN does not need acceptor doping, and can be formed entirely by the difference in the internal polarization fields across the semiconductor heterojunction. The measured 2D hole gas densities, about {\$}4 \backslashtimes 10{\^{}}{\{}13{\}}{\$} cm{\$}{\^{}}{\{}-2{\}}{\$}, are among the highest among all known semiconductors and remain unchanged down to cryogenic temperatures. Some of the lowest sheet resistances of all wide-bandgap semiconductors are seen. The observed results provide a new probe for studying the valence band structure and transport properties of wide-bandgap nitride interfaces, and simultaneously enable the missing component for gallium nitride-based p-channel transistors for energy-efficient electronics.},
archivePrefix = {arXiv},
arxivId = {1807.08836},
author = {Chaudhuri, Reet and Bader, Samuel James and Chen, Zhen and Muller, David A. and Xing, Huili and Jena, Debdeep},
eprint = {1807.08836},
file = {:Users/Aushickman/Documents/Cornell/Papers/QWHEMT{\_}breakdown{\_}bib/A polarization-induced 2D hole gas in undoped gallium nitride quantum wells.pdf:pdf},
title = {{A polarization-induced 2D hole gas in undoped gallium nitride quantum wells}},
url = {http://arxiv.org/abs/1807.08836},
year = {2018}
}
@article{Rennesson2018,
abstract = {{\textcopyright} 2017 WILEY-VCH Verlag GmbH  {\&}  Co. KGaA, Weinheim. AlN-based HEMTs grown on silicon by ammonia-assisted molecular beam epitaxy (NH 3 -MBE) are demonstrated and studied. As shown by photoluminescence, the very thin GaN channel (35-55nm-thick) is compressively strained on the 250nm-thick relaxed AlN buffer layer grown on silicon substrate. The structure is then completed by an 8nm-thick AlN barrier and 2nm-thick GaN cap. Despite an ultrathin total epilayer (only ≈300nm-thick), a high 2DEG density (≈2.7×10 13 cm -2 ) is measured by Hall effect. Room temperature mobility values, as high as 636cm 2 V -1 s -1 , are measured. They are higher than those reported on similar structures grown on sapphire, SiC and bulk AlN substrates, showing the interest of growing such structures on silicon substrate. Also, low contact resistance values are obtained, as low as 0.18$\Omega$mm, by using a basic fabrication process.},
author = {Rennesson, Stephanie and Leroux, Mathieu and {Al Khalfioui}, Mohamed and Nemoz, Maud and Chenot, S{\'{e}}bastien and Massies, Jean and Largeau, Ludovic and Dogmus, Ezgi and Zegaoui, Malek and Medjdoub, Farid and Semond, Fabrice},
doi = {10.1002/pssa.201700640},
file = {:Users/Aushickman/Documents/Cornell/Papers/QWHEMT{\_}breakdown{\_}bib/Rennesson{\_}et{\_}al-2018-physica{\_}status{\_}solidi{\_}(a).pdf:pdf},
issn = {18626319},
journal = {Physica Status Solidi (A) Applications and Materials Science},
keywords = {AlN,GaN,compressive strain,high electron mobility transistors,ohmic contacts,silicon},
number = {9},
pages = {1--4},
title = {{Ultrathin AlN-Based HEMTs Grown on Silicon Substrate by NH3-MBE}},
volume = {215},
year = {2018}
}
